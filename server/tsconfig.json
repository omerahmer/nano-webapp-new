{
    "compilerOptions": {
        "target": "es6",
        "module": "CommonJS",
        "outDir": "static",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}